{
  "name": "fruit-guardians-vitepress-template",
  "version": "1.0.0",
  "description": "一个现代化、美观、易用的VitePress文档模板，专为开发者和团队打造。",
  "type": "module",
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "preview": "vitepress preview docs",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "deploy": "npm run build && touch docs/.vitepress/dist/.nojekyll",
    "clean": "rimraf docs/.vitepress/dist",
    "type-check": "vue-tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fruit-Guardians/Vitrpress-theme.git"
  },
  "keywords": [
    "vitepress",
    "template",
    "docs",
    "modern",
    "vue3",
    "tailwindcss",
    "element-plus",
    "文档模板",
    "现代化"
  ],
  "author": "Fruit-Guardians,bx",
  "license": "MIT",
  "homepage": "https://github.com/Fruit-Guardians/Vitrpress-theme",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.3.0",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^9.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "postcss": "^8.4.21",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitepress": "1.2.3",
    "vue-tsc": "^2.0.0"
  },
  "dependencies": {
    "element-plus": "^2.2.32",
    "medium-zoom": "^1.0.8",
    "vue": "^3.4.14"
  }
}
